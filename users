package Login;

public class users {
String username;
String password;
users()
{
	username=null;
	password=null;
}
users(String a,String b)
{
	username=a;
	password=b;
}
void setUsername(String s)
{
	username=s;
}
void setPassword(String s)
{
	password=s;
}
String getUsername()
{
	return username;
}
String getPassword()
{
	return password;
}
}
